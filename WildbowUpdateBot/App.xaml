<Application x:Class="WildbowUpdateBot.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WildbowUpdateBot" xmlns:sys="clr-namespace:System;assembly=System.Runtime"
             Startup="Application_Startup">
    <Application.Resources>
        <sys:String x:Key="CurrentSerialURI">palewebserial.wordpress.com</sys:String>
        <sys:UInt32 x:Key="CurrentTimeInterval">1</sys:UInt32>
        <sys:Int64 x:Key="LastSeenPageID">-1</sys:Int64>
        <sys:Boolean x:Key="AutostartApp">True</sys:Boolean>
        <sys:UInt32 x:Key="AlertVolume">50</sys:UInt32>
    </Application.Resources>
</Application>
